<h1> Welcome <%= current_user.email %></h1>
  <div class='row'>
    <div class='col-6'>
      <div class='card'>
        <div class="card-block carddb">
        <h4 class='card-title'>Total Database</h4>
        <%= link_to 'Mange People', people_path, class: 'btn btn-primary'%>
        <br>
        <strong>People:</strong> <%= @people.length %>
        </div>
      </div>
    </div>
    <div class='col-6'>
      <div class='card'>
        <div class="card-block carddb">
          <h4 class='card-title'>Tracking</h4>
          <strong>Person added</strong>
            <%= line_chart Person.group_by_day(:created_at).count %>

          <strong>People attend</strong>

      </div>
      </div>
    </div>
  </div>
  <section class='home-information'>
    <h4>Real Time Events</h4>
    <small>Last 20 events</small>
  </section>
