<p id="notice"><%= notice %></p>

<h1>Booths</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Name</th>
      <th>Start</th>
      <th>End</th>
      <th>Sponser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booths.each do |booth| %>
      <tr>
        <td><%= booth.event.name %></td>
        <td><%= booth.name %></td>
        <td><%= booth.start %></td>
        <td><%= booth.end %></td>
        <td><%= booth.sponser %></td>
        <td><%= link_to 'Show', event_booth_path(booth.event.id, booth) %></td>
        <td><%= link_to 'Edit', edit_event_booth_path(booth.event.id,booth) %></td>
        <td><%= link_to 'Destroy', event_booths_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booth', new_event_booth_path %>
